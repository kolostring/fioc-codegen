{
  "name": "@fioc/codegen",
  "version": "0.1.0",
  "description": "Automatic DI token and container generator for FIoC",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "fioc-generate": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run typecheck && pnpm run build && pnpm test --run",
    "release": "pnpm run build && pnpm test --run && pnpm publish --access public"
  },
  "keywords": [
    "dependency-injection",
    "di",
    "ioc",
    "fioc",
    "code-generation",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "ts-morph": "^27.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
